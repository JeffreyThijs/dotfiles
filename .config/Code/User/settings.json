{
    "workbench.colorTheme": "Atom One Dark",
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#000ecc",
        "editor.selectionHighlightBackground": "#9b0000"
    },
    "window.zoomLevel": 0,
    "workbench.startupEditor": "newUntitledFile",
    "files.associations": {
        "*.ts": "typescript"
    },
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 0}",
    "python.pythonPath": "/usr/bin/python3",
    "python.showStartPage": false,
    "python.languageServer": "Pylance",
    "jupyter.debugJustMyCode": false,
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "python.analysis.diagnosticMode": "workspace",
    "python.linting.flake8Enabled": true,
    "python.linting.enabled": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.configuration.runtimes": [
        {
          "name": "JavaSE-11",
          "path": "/home/jthijs/Documents/jdk-11.0.10+9/",
        }
    ],
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "quarkus.tools.debug.terminateProcessOnExit": "Always terminate",
    "java.project.importHint": false,
    "java.project.importOnFirstTimeStartup": "automatic",
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },
    "redhat.telemetry.enabled": true,
    "quarkus.tools.alwaysShowWelcomePage": false,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "vsicons.dontShowNewVersionMessage": true,
    "debug.onTaskErrors": "debugAnyway",
    "[jsonc]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "python.analysis.typeCheckingMode": "basic",
    "prettier.tabWidth": 4,
    "prettier.useTabs": true,
    "java.codeGeneration.useBlocks": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "helm-intellisense.customValueFileNames": [
        "values.yaml",
        "values-grafana.yml"
    ]
}
